{
	"name": "@beroboard/db",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./src/index.ts"
		},
		"./types": {
			"types": "./dist/types.d.ts",
			"default": "./src/types.ts"
		},
		"./*": {
			"types": "./dist/*.d.ts",
			"default": "./src/*.ts"
		}
	},
	"scripts": {
		"build": "tsdown",
		"db:push": "prisma db push",
		"db:studio": "prisma studio",
		"db:generate": "prisma generate",
		"db:migrate": "prisma migrate deploy",
		"db:migrate:create": "bunx prisma migrate dev --name"
	},
	"devDependencies": {
		"prisma": "^6.15.0",
		"tsdown": "catalog:",
		"@beroboard/config": "workspace:*"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@prisma/client": "catalog:",
		"dotenv": "catalog:",
		"zod": "catalog:"
	}
}
